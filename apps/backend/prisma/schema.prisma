generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Activity {
  id        Int      @id @default(autoincrement())
  sport     String
  duration  Int
  xp        Int
  createdAt DateTime @default(now())
}

model Test {
  id        Int      @id @default(autoincrement())
  content   String
  createdAt DateTime @default(now())
}
